<table class="table table-striped">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Date</th>
                  <th>Times</th>
                </tr>
              </thead>
              <tbody>
{{range .}}
                <tr>
                  <td>{{.Name}}</td>
                  <td>{{.Date}}</td>
                  <td>{{.Times}}</td>
                </tr>
{{ end }}
              </tbody>
            </table>
